{
	"elements":
			[
				{ "type": "ValidationTextBox", "name": "user", "caption": "User" },
				{ "type": "ValidationTextBox", "name": "password", "caption": "Password" }
			],
	"actions":
			[
				{ "type": "Button", "label": "Test account", "onClick": "Sipgate_TestAccount($id);" },

				{ "type": "Label", "label": "" },
				{ "type": "ValidationTextBox", "name": "telno", "caption": "telno" },
				{ "type": "ValidationTextBox", "name": "msg", "caption": "msg" },
				{ "type": "Button", "label": "Test SMS", "onClick": "Sipgate_TestSMS($id, $telno, $msg);" },

				{ "type": "Button", "label": "Show Call-History", "onClick": "Sipgate_ShowHistory($id);" },

				{ "type": "Button", "label": "Show current Calls", "onClick": "Sipgate_ShowCallList($id);" },

				{ "type": "Button", "label": "Show current Forwardings", "onClick": "Sipgate_ShowForwardings($id);" },

				{ "type": "ValidationTextBox", "name": "destination", "caption": "destination" },
				{ "type": "NumberSpinner", "name": "timeout", "caption": "timeout" },
				{ "type": "CheckBox", "name": "active", "caption": "active" },
				{ "type": "Button", "label": "Test Forwarding", "onClick": "Sipgate_TestForwarding($id, $destination, $timeout, $active);" },
				{ "type": "Label", "label": "____________________________________________________________________________________________________" },
				{ "type": "Button", "label": "Module description", "onClick": "echo 'https://github.com/demel42/IPSymconSipgate/blob/master/README.md';" }
			],		
	"status":
			[
				{ "code": 101, "icon": "inactive", "caption": "Instance getting created" },
				{ "code": 102, "icon": "active", "caption": "Instance is active" },
				{ "code": 104, "icon": "inactive", "caption": "Instance is inactive" },

				{ "code": 201, "icon": "error", "caption": "Instance is inactive (unauthorized)" },
				{ "code": 202, "icon": "error", "caption": "Instance is inactive (server error)" },
				{ "code": 203, "icon": "error", "caption": "Instance is inactive (http error)" },
				{ "code": 204, "icon": "error", "caption": "Instance is inactive (invalid data)" }
			]
}
